(ns social-centrality.test
  (:use clojure.test))

(deftest k-merge
  (testing "Merging"
    (testing "when intermediate vertice"
      (testing "has shorter distance paths"
        (is (= {:2 {:1 4 :3 2}} (k-merge {:1 {:3 -3}} {:2 {:1 4, :3 3}})))
      (testing "does not contain shorter distance paths")
      (testing "has a new path")
    
)))
